@model WebApp.DTO_Models.Final.CertificateExaminationView

<head>
    <link rel="stylesheet" href="~/css/QuestionDetails.css" asp-append-version="true" />
</head>
<div class="left"><h2>Exam Id: @Model.ExaminationId</h2></div>

<table class="table table-hover width=30% table-light" style="text-align: center">
    <tr>
        <th>Certficate Title</th>
        <td>@Model.CertificateTitle</td>
    </tr>
    <tr>
        <th>Questions</th>
        
            @foreach (var question in Model.Questions)
            {
            <td>@Html.Raw(question.QuestionDisplay)</td>
            }
        
    </tr>
</table>

    <div>
        <a asp-action="CertificateExaminationsIndex">Back to List</a>
    </div>
